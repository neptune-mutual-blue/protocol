{
  "editor.tabSize": 2,
  "editor.foldingStrategy": "indentation",
  "editor.wrappingIndent": "indent",
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.autoIndent": "none",
  "solidity.formatter": "prettier",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "solidity.compileUsingRemoteVersion": "0.8.15",
  "solidity.linter": "solhint",
  "solidity.validationDelay": 2,
  "standard.autoFixOnSave": true,
  "javascript.suggestionActions.enabled": false,
  "tsImportSorter.configuration.development.enableDebug": false,
  "tsImportSorter.configuration.hasSemicolon": false,
  "tsImportSorter.configuration.trailingComma": "none",
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "standard.vscode-standard"
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "vscode.json-language-features",
  },
  "editor.padding.top": 24,
  "terminal.integrated.scrollback": 50000,
  "cSpell.allowCompoundWords": true,
  "cSpell.enabledLanguageIds": [
    "javascript",
    "json",
    "jsonc",
    "markdown",
    "plaintext",
    "text",
    "typescript",
    "yaml",
    "yml",
    "solidity"
  ],
  "cSpell.words": [
    "Aave",
    "alloc",
    "Arbitrum",
    "Bahs",
    "Bokky",
    "BSCSCAN",
    "CDAI",
    "chainid",
